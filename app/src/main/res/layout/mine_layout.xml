<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/my">


<RelativeLayout

    android:id="@+id/relative"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_350"
    android:layout_alignParentBottom="true"
    android:background="@drawable/wode_background">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_350"
        android:layout_alignParentBottom="true">

        <ImageView
            android:id="@+id/img_one"
            android:layout_width="@dimen/dp_50"
            android:layout_height="@dimen/dp_50"
            android:layout_marginTop="@dimen/dp_60"
            android:layout_marginLeft="@dimen/dp_10"
            android:src="@drawable/one" />

        <EditText
            android:id="@+id/ed_one"
            android:layout_width="@dimen/dp_250"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/dp_10"
            android:layout_alignParentRight="true"
            android:hint="个人资料"
            android:layout_marginTop="@dimen/dp_70"/>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_350"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="4dp">

        <ImageView
            android:id="@+id/img_two"
            android:layout_width="@dimen/dp_50"
            android:layout_height="@dimen/dp_50"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_120"
            android:src="@drawable/two" />

        <EditText
            android:id="@+id/ed_two"
            android:layout_width="@dimen/dp_250"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_120"
            android:layout_marginRight="@dimen/dp_10"
            android:hint="我的圈子" />

    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_350"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="4dp">

        <ImageView
            android:id="@+id/img_three"
            android:layout_width="@dimen/dp_50"
            android:layout_height="@dimen/dp_50"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_180"
            android:src="@drawable/three" />

        <EditText
            android:id="@+id/ed_three"
            android:layout_width="@dimen/dp_250"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_180"
            android:layout_marginRight="@dimen/dp_10"
            android:hint="我的足迹" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_350"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="4dp">

        <ImageView
            android:id="@+id/img_fore"
            android:layout_width="@dimen/dp_50"
            android:layout_height="@dimen/dp_50"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_240"
            android:src="@drawable/fore" />

        <EditText
            android:id="@+id/ed_fore"
            android:layout_width="@dimen/dp_250"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_240"
            android:layout_marginRight="@dimen/dp_10"
            android:hint="我的钱包" />

    </RelativeLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_350"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="3dp">

        <ImageView
            android:id="@+id/img_five"
            android:layout_width="@dimen/dp_50"
            android:layout_height="@dimen/dp_50"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_300"
            android:src="@drawable/five" />

        <EditText
            android:id="@+id/ed_five"
            android:layout_width="@dimen/dp_250"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_300"
            android:layout_marginRight="@dimen/dp_10"
            android:hint="我的收货地址" />

    </RelativeLayout>
</RelativeLayout>
    <ImageView
        android:id="@id/img_tou"
        android:layout_width="@dimen/dp_120"
        android:layout_height="@dimen/dp_120"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dp_70"
        android:src="@drawable/my_yuan" />
    <TextView
        android:id="@+id/text_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="吃不胖的小姐姐"
        android:layout_below="@+id/img_tou"
        android:layout_marginLeft="@dimen/dp_140"
        android:layout_marginTop="@dimen/dp_10"

         />
</RelativeLayout>
